<div class="container home-page">
<button onclick="typeWriter()" class="period"> <i class="fa-solid fa-circle fa-2xl" style="color: #85ff93;"></i> </button>

<p id="demo"></p>

<script>
var i = 0;
var txt = 'Are you the most sustainable in your circle? Lets make you.';
var speed = 50;

function typeWriter() {
  if (i < txt.length) {
    document.getElementById("demo").innerHTML += txt.charAt(i);
    i++;
    setTimeout(typeWriter, speed);
  }
}
</script>
</div>
